function joi

push data 0 "Enter your name: " STR
print data 0 STR

scan local 0 STR

push data 1 "Hello! Enter temperature in Celsius: " STR
print data 1 STR

scan local 1 FLOAT

push local 1 FLOAT
push constant 9.0 FLOAT 
mul FLOAT
pop temp 0 FLOAT

push temp 0 FLOAT
push constant 5.0 FLOAT
div FLOAT
pop temp 1 FLOAT

push temp 1 FLOAT
push constant 32.0 FLOAT
add FLOAT
pop local 2 FLOAT

push local 2 FLOAT
print FLOAT

push constant 0 INT 
return INT
