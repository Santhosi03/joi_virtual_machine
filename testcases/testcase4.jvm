Square_constructor:
    IPUSH FP
    MOV FP, SP
    
    // Store vtable pointer
    IPUSH VTABLE_Square
    SETFIELD 0
    
    // Store side
    LOAD [FP-4]
    SETFIELD 4
    
    MOV SP, FP
    IPOP FP
    RET

Square_area:
    IPUSH FP
    MOV FP, SP
    
    GETFIELD 4      // Get side
    IDUP             // Duplicate for multiplication
    IMUL             // Calculate area
    
    MOV SP, FP
    IPOP FP
    RET